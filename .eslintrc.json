{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "linebreak-style": "off",
        "no-unused-vars": "warn",
        "no-console": "warn",
        "arrow-body-style": "off",
        "no-param-reassign": "off"
    }
}
